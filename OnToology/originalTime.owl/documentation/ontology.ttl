@prefix :        <http://www.w3.org/2006/time#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time:    <http://www.w3.org/2006/time#> .
@prefix tzont:   <http://www.w3.org/2006/timezone#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

time:intervalEquals
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval .

time:unitWeek
      a       time:TemporalUnit .

time:Monday
      a       time:DayOfWeek .

time:days
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

tzont:TimeZone
      a       owl:Class .

time:before
      a       owl:TransitiveProperty , owl:ObjectProperty ;
      rdfs:domain time:TemporalEntity ;
      rdfs:range time:TemporalEntity .

time:unitYear
      a       time:TemporalUnit .

time:intervalBefore
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval ;
      rdfs:subPropertyOf time:before .

time:hasBeginning
      a       owl:ObjectProperty ;
      rdfs:domain time:TemporalEntity ;
      rdfs:range time:Instant .

time:hours
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

time:xsdDateTime
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeInterval ;
      rdfs:range xsd:dateTime .

time:Thursday
      a       time:DayOfWeek .

time:minute
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:nonNegativeInteger .

<http://www.w3.org/2006/time>
      a       owl:Ontology ;
      rdfs:comment """
      An OWL Ontology of Time (OWL-Time).
    """ ;
      rdfs:seeAlso <http://www.w3.org/TR/owl-time> .

time:Saturday
      a       time:DayOfWeek .

time:January
      a       owl:Class ;
      rdfs:subClassOf time:DateTimeDescription ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:hasValue "--01"^^xsd:gMonth ;
                owl:onProperty time:month
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:hasValue time:unitMonth ;
                owl:onProperty time:unitType
              ] .

time:unitMonth
      a       time:TemporalUnit .

time:inDateTime
      a       owl:ObjectProperty ;
      rdfs:domain time:Instant ;
      rdfs:range time:DateTimeDescription .

time:intervalAfter
      a       owl:ObjectProperty ;
      owl:inverseOf time:intervalBefore .

time:year
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:gYear .

time:Instant
      a       owl:Class ;
      rdfs:subClassOf time:TemporalEntity .

time:intervalContains
      a       owl:ObjectProperty ;
      owl:inverseOf time:intervalDuring .

time:intervalOverlaps
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval .

time:years
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

time:hasDurationDescription
      a       owl:ObjectProperty ;
      rdfs:domain time:TemporalEntity ;
      rdfs:range time:DurationDescription .

time:dayOfWeek
      a       owl:ObjectProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range time:DayOfWeek .

time:DayOfWeek
      a       owl:Class ;
      owl:oneOf (time:Sunday time:Monday time:Tuesday time:Wednesday time:Thursday time:Friday time:Saturday) .

time:unitHour
      a       time:TemporalUnit .

time:intervalMetBy
      a       owl:ObjectProperty ;
      owl:inverseOf time:intervalMeets .

time:seconds
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

time:TemporalEntity
      a       owl:Class ;
      owl:unionOf (time:Instant time:Interval) .

time:weeks
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

time:unitDay
      a       time:TemporalUnit .

time:intervalStarts
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval .

time:intervalFinishes
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval .

time:intervalFinishedBy
      a       owl:ObjectProperty ;
      owl:inverseOf time:intervalFinishes .

time:DateTimeInterval
      a       owl:Class ;
      rdfs:subClassOf time:ProperInterval .

time:Sunday
      a       time:DayOfWeek .

time:intervalDuring
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval .

time:unitSecond
      a       time:TemporalUnit .

time:Interval
      a       owl:Class ;
      rdfs:subClassOf time:TemporalEntity .

time:Wednesday
      a       time:DayOfWeek .

time:unitType
      a       owl:ObjectProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range time:TemporalUnit .

time:Friday
      a       time:DayOfWeek .

time:months
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

time:Year
      a       owl:Class ;
      rdfs:subClassOf time:DurationDescription ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty time:seconds
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty time:days
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty time:hours
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty time:minutes
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty time:months
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:years
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "0"^^xsd:nonNegativeInteger ;
                owl:onProperty time:weeks
              ] .

time:hour
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:nonNegativeInteger .

time:DateTimeDescription
      a       owl:Class ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:cardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:unitType
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:day
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:second
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:dayOfWeek
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:week
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:minute
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:timeZone
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:month
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:year
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:dayOfYear
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:hour
              ] .

time:minutes
      a       owl:DatatypeProperty ;
      rdfs:domain time:DurationDescription ;
      rdfs:range xsd:decimal .

time:ProperInterval
      a       owl:Class ;
      rdfs:subClassOf time:Interval ;
      owl:disjointWith time:Instant .

time:DurationDescription
      a       owl:Class ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:minutes
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:months
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:years
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:days
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:weeks
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:seconds
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                owl:onProperty time:hours
              ] .

time:second
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:decimal .

time:Tuesday
      a       time:DayOfWeek .

time:intervalMeets
      a       owl:ObjectProperty ;
      rdfs:domain time:ProperInterval ;
      rdfs:range time:ProperInterval .

time:inside
      a       owl:ObjectProperty ;
      rdfs:domain time:Interval ;
      rdfs:range time:Instant .

time:intervalOverlappedBy
      a       owl:ObjectProperty ;
      owl:inverseOf time:intervalOverlaps .

time:dayOfYear
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:nonNegativeInteger .

time:hasDateTimeDescription
      a       owl:ObjectProperty ;
      rdfs:domain time:DateTimeInterval ;
      rdfs:range time:DateTimeDescription .

time:hasEnd
      a       owl:ObjectProperty ;
      rdfs:domain time:TemporalEntity ;
      rdfs:range time:Instant .

time:inXSDDateTime
      a       owl:DatatypeProperty ;
      rdfs:domain time:Instant ;
      rdfs:range xsd:dateTime .

time:month
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:gMonth .

time:unitMinute
      a       time:TemporalUnit .

time:day
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:gDay .

time:TemporalUnit
      a       owl:Class ;
      owl:oneOf (time:unitSecond time:unitMinute time:unitHour time:unitDay time:unitWeek time:unitMonth time:unitYear) .

time:timeZone
      a       owl:ObjectProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range tzont:TimeZone .

time:intervalStartedBy
      a       owl:ObjectProperty ;
      owl:inverseOf time:intervalStarts .

time:after
      a       owl:ObjectProperty ;
      owl:inverseOf time:before .

time:week
      a       owl:DatatypeProperty ;
      rdfs:domain time:DateTimeDescription ;
      rdfs:range xsd:nonNegativeInteger .
